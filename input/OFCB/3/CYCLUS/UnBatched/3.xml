<!-- 1 Source Source, 1 Sink Sink -->

<simulation>
    <control>
        <duration>1200</duration>
        <startmonth>1</startmonth>
        <startyear>2000</startyear>
    </control>

    <archetypes>
        <spec><lib>cycamore</lib><name>Sink</name></spec>
        <spec><lib>cycamore</lib><name>Source</name></spec>
        <spec><lib>cycamore</lib><name>Reactor</name></spec>
        <spec><lib>cycamore</lib><name>Enrichment</name></spec>
        <spec><lib>agents</lib><name>NullRegion</name></spec>
        <spec><lib>agents</lib><name>NullInst</name></spec>
    </archetypes>

    <facility>
        <name>SourceUOX</name>
        <config>
            <Source>
            <outcommod>UOX</outcommod>
            <outrecipe>UOX</outrecipe>
            <throughput>1000</throughput>
            </Source>
        </config>
    </facility>

     <!--
        <facility>
        <name>Enrichment</name>
         <config>
            <Enrichment>
            <feed_commod>natl_u</feed_commod>
            <product_commod>enriched_u</product_commod>
            <tails_commod>ef_tails</tails_commod>
            <feed_recipe>natl_u</feed_recipe>
            <tails_assay>0.003</tails_assay>
            <max_feed_inventory>500</max_feed_inventory>
            </Enrichment>
        </config>
        </facility>
    -->

    <facility>
        <name>Reactor</name>
        <config>
            <Reactor>
            <fuel_inrecipes>    <val>UOX</val>        </fuel_inrecipes>
            <fuel_outrecipes>    <val>SpentUOX</val>    </fuel_outrecipes>
            <fuel_incommods>    <val>UOX</val>        </fuel_incommods>
            <fuel_outcommods>    <val>SpentUOX</val>    </fuel_outcommods>

            <cycle_time>3</cycle_time>
            <power_cap>965.16</power_cap>
            <refuel_time>0</refuel_time>
            <assem_size>82</assem_size>
            <n_assem_core>1</n_assem_core>
            <n_assem_spent>1</n_assem_spent>
            <n_assem_batch>1</n_assem_batch>
            </Reactor>
        </config>
    </facility>

    <facility>
        <name>Sink</name>
        <config>
            <Sink>
            <in_commods>
                <val>SpentUOX</val>
            </in_commods>
            <capacity>100000000</capacity>
            </Sink>
        </config>
    </facility>

    <region>
        <name>SingleRegion</name>
        <config><NullRegion/></config>
        <institution>
            <name>SingleInstitution</name>
            <initialfacilitylist>
            <entry>
            <prototype>SourceUOX</prototype>
                <number>1</number>
            </entry>
            <entry>
                <prototype>Reactor</prototype>
                <number>10</number>
            </entry>
            <entry>
                <prototype>Sink</prototype>
                <number>1</number>
            </entry>
            </initialfacilitylist> 
            <config><NullInst/></config>
        </institution>
    </region>

    <recipe>
        <name>UOX</name>
        <basis>mass</basis>
        <nuclide>
            <id>922350000</id>
            <comp>3.25</comp>
        </nuclide>
        <nuclide>
            <id>922380000</id>
            <comp>96.75</comp>
        </nuclide>
        <nuclide>
            <id>80160000</id>
            <comp>200</comp>
        </nuclide>
    </recipe>

    <recipe>
        <name>SpentUOX</name>
        <basis>atom</basis>
        <nuclide><id>922350000</id>    <comp>0.0003</comp></nuclide>
        <nuclide><id>922350000</id>    <comp>0.5248</comp></nuclide>
        <nuclide><id>922360000</id>    <comp>0.4140</comp></nuclide>
        <nuclide><id>922380000</id>    <comp>89.3300</comp></nuclide>

        <nuclide><id>932370000</id>    <comp>0.0526</comp></nuclide>

        <nuclide><id>942380000</id>    <comp>0.0229</comp></nuclide>
        <nuclide><id>942390000</id>    <comp>0.5429</comp></nuclide>
        <nuclide><id>942400000</id>    <comp>0.2472</comp></nuclide>
        <nuclide><id>942410000</id>    <comp>0.1592</comp></nuclide>
        <nuclide><id>942420000</id>    <comp>0.0825</comp></nuclide>

        <nuclide><id>952410000</id>    <comp>0.0040</comp></nuclide>
        <nuclide><id>952420000</id>    <comp>0.0001</comp></nuclide>
        <nuclide><id>952430000</id>    <comp>0.0189</comp></nuclide>

        <nuclide><id>962420000</id>    <comp>0.0021</comp></nuclide>
        <nuclide><id>962430000</id>    <comp>0.0001</comp></nuclide>
        <nuclide><id>962440000</id>    <comp>0.0077</comp></nuclide>
        <nuclide><id>962450000</id>    <comp>0.0005</comp></nuclide>

        <nuclide><id>80160000</id>    <comp>200</comp></nuclide>
    </recipe>

</simulation>






